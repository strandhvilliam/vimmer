{
  "name": "vimmer",
  "private": true,
  "scripts": {
    "check-aws-account": "if [ \"$(aws sts get-caller-identity --query 'Account' --output text)\" = \"975050331958\" ]; then echo '✅ Correct AWS account'; else echo '❌ Wrong AWS account, aborting.' && exit 1; fi",
    "dev": "bun check-aws-account && sst dev --stage development",
    "deploy:prod": "bun check-aws-account && sst deploy --stage production",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@effect/vitest": "^0.26.0",
    "@types/aws-lambda": "8.10.146",
    "prettier": "^3.5.3",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.35",
  "workspaces": [
    "apps/*",
    "packages/*",
    "services/*",
    "tasks/*"
  ],
  "dependencies": {
    "@aws-sdk/lib-storage": "^3.828.0",
    "@trpc/client": "^11.4.2",
    "@trpc/server": "^11.4.2",
    "@trpc/tanstack-react-query": "^11.4.2",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "effect": "^3.17.9",
    "react": "19.1",
    "sharp": "^0.34.3",
    "sst": "^3.17.10"
  }
}
